{
  "name": "next-360-image-viewer",
  "description": "Display a set of images in a draggable 360 degree viewer.",
  "keywords": [
    "nextjs",
    "360",
    "image",
    "viewer"
  ],
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ImmersiveStudio/next-image-turntable.git"
  },
  "author": "Alton Cuff",
  "license": "MIT",
  "sideEffects": false,
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "bootstrap": "corepack disable && corepack enable && npm i --frozen-lockfile",
    "build": "tsup",
    "dev": "npm -r --stream run start",
    "dx": "npm run bootstrap && npm exec husky install && npm run prepare",
    "lint": "eslint . && prettier --check .",
    "prepare": "husky install",
    "release": "npm run build && np --no-tests --no-cleanup",
    "size": "size-limit",
    "start": "tsup --watch"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix"
    ]
  },
  "np": {
    "npm": true
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "1.2 KB"
    }
  ],
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@playwright/test": "^1.21.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest-axe": "^3.5.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "axe-core": "^4.4.1",
    "axe-playwright": "^1.1.11",
    "babel-plugin-istanbul": "^6.1.1",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.15.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "np": "^7.6.1",
    "nyc": "^15.1.0",
    "playwright-core": "^1.21.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tslib": "^2.4.0",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "next": "^13.1.5"
  }
}
